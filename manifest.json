{
  "manifest_version": 2,
  "name": "PoolTogether Extention Login",
  "version": "1.0.0",
  "description": "Secure Redirect To PoolTogether",
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
    }
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}
